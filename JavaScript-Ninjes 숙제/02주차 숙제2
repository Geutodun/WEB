let num=5
for(let i=0;i<num;i++){
    for(int j=num-1;j>i;j--){
	console.log(" ")
    }
		    
    for(let j=0;j<2*i+1;j++){
	    console.log("*")
    }
    console.log("\n")
}
	
for(let i=1;i<num;i++){
   for(let j=0;j<i;j++){
	 console.log(" ")
   }
	
    for(let j=2*num-1;j>2*i;j--){
         console.log("*")
    }
    console.log("\n")
}
